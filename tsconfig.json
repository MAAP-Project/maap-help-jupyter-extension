{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "composite": true,
    "declaration": true,
    "esModuleInterop": true,
    "incremental": true,
    "lib": ["esnext", "dom"],
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "preserveWatchOutput": true,
    "resolveJsonModule": true,
    "outDir": "lib",
    "rootDir": "src",
    "strict": true,
    "strictNullChecks": true,
    "target": "es2017",
    "types": ["jest"]
  },
  "include": ["src/*", "src/jupyterlab-tour/*"]// "src/docmanager/src/testutils/*", "src/docmanager/src/testutils/session/*", "src/docmanager/src/testutils/kernelspec/*", "src/docmanager/src/testutils/kernel/*", "src/docmanager/src/testutils/contents/*"]
}
